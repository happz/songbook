{
   "chords" : [],
   "diagnostics" : {
      "format" : "\"%f\", line %n, %m\n\t%l"
   },
   "metadata" : {
      "keys" : [
         "title",
         "subtitle",
         "artist",
         "composer",
         "lyricist",
         "arranger",
         "album",
         "copyright",
         "year",
         "key",
         "time",
         "tempo",
         "capo",
         "duration",
         "source"
      ],
      "separator" : "; ",
      "strict" : true
   },
   "toc" : {
     "title" : "Table of Contents",
     "order": "page"
   },
   "pdf" : {
      "chordscolumn" : 0,
      "chorus" : {
         "bar" : {
            "color" : "black",
            "offset" : 8,
            "width" : 1
         },
         "indent" : 0,
         "recall" : {
            "quote" : false,
            "tag" : "R:",
            "type" : "comment"
         }
      },
      "columnspace" : 20,
      "even-odd-pages" : 1,
      "fontdir": [
        "/usr/share/fonts/liberation",
        "/usr/share/fonts/liberation-fonts"
      ],
      "fonts" : {
         "chord" : {
            "file" : "LiberationSerif-BoldItalic.ttf",
            "size" : 12
         },
         "comment" : {
            "file" : "LiberationSerif-Regular.ttf",
            "size" : 12,
            "background": "white"
         },
         "tab" : {
            "file" : "LiberationMono-Regular.ttf",
            "size" : 10
         },
         "text" : {
            "file" : "LiberationSerif-Regular.ttf",
            "size" : 12
         },
         "title" : {
            "file" : "LiberationSerif-Bold.ttf",
            "size" : 22
         },
         "toc" : {
            "file" : "LiberationSerif-Regular.ttf",
            "size" : 11
         }
      },
      "footspace" : 20,
      "formats" : {
         "default" : {
            "footer" : [
               "%{title}",
               "",
               "%{page}"
            ],
            "subtitle" : null,
            "title" : null
         },
         "first" : {
            "footer" : null
         },
         "title" : {
            "footer" : [
               "",
               "",
               "%{page}"
            ],
            "subtitle" : [
               "",
               "%{subtitle}",
               ""
            ],
            "title" : [
               "",
               "%{title}",
               ""
            ]
         }
      },
      "head-first-only" : false,
      "headspace" : 60,
      "marginbottom" : 40,
      "marginleft" : 40,
      "marginright" : 40,
      "margintop" : 80,
      "papersize" : "a4",
      "showlayout" : false,
      "spacing" : {
         "chords" : 1.2,
         "empty" : 1,
         "grid" : 1.2,
         "lyrics" : 1.2,
         "tab" : 1,
         "title" : 1.2,
         "toc" : 1.4
      },
      "titles-directive-ignore" : false
   },
   "settings" : {
      "columns" : 1,
      "lyrics-only" : false,
      "suppress-empty-chords" : true,
      "titles" : "left"
   }
}
